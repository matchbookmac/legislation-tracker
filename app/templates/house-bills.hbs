<div class="col-md-8 col-md-offset-2">
  <ul>
    {{#each bill in model}}
      <li>
        Bill ID:
        {{#link-to 'bill' bill.bill_id}}
          <span class="bill-name">{{bill.bill_id}}</span>
        {{/link-to}}
        <br>
        Bill Description:
        <span class="bill-description">{{bill.description}}</span>
      </li>
    {{/each}}
  </ul>
</div>
